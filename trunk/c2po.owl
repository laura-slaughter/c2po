    <!-- http://www.ifomis.org/acgt/1.0#Pharmacotherapy -->

    <owl:Class rdf:about="&acgt;Pharmacotherapy">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&obo;OGMS_0000090"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&ro;has_participant"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&snap;MaterialEntity"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="&OBO_REL;bearer_of"/>
                                        <owl:someValuesFrom rdf:resource="&obo;OBI_0000040"/>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&obo;OGMS_0000090"/>
        <obo:IAO_0000115 xml:lang="en">A treatment involving the administration of a drug.</obo:IAO_0000115>
        <rdfs:comment xml:lang="en">imported from ACGT</rdfs:comment>
    </owl:Class>
    


